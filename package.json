{
  "name": "vscode-datetime",
  "displayName": "DateTime",
  "description": "This extension outputs the date and time as formatted text",
  "version": "0.3.0",
  "publisher": "SatoshiYamamoto",
  "author": {
    "name": "Satoshi Yamamoto",
    "email": "standard.software.net@gmail.com",
    "url": "https://www.facebook.com/stndardsoftware"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-software/vscode-datetime"
  },
  "engines": {
    "vscode": "^1.62.3"
  },
  "categories": [
    "Other"
  ],
  "keywords": ["datetime"],
  "bugs": {
    "url": "https://github.com/standard-software/vscode-datetime/issues"
  },
  "main": "./src/extension.js",
  "activationEvents": [
    "onCommand:DateTime.InsertDateTodayDefaultFormat",
    "onCommand:DateTime.InsertDateTimeNowDefaultFormat",
    "onCommand:DateTime.InsertTimeNowDefaultFormat",
    "onCommand:DateTime.SelectFunction"
  ],
  "contributes": {
    "commands": [
      {
        "command": "DateTime.InsertDateTodayDefaultFormat",
        "title": "DateTime | Insert | Date Today | Default Format"
      },
      {
        "command": "DateTime.InsertDateTimeNowDefaultFormat",
        "title": "DateTime | Insert | DateTime Today Now | Default Format"
      },
      {
        "command": "DateTime.InsertTimeNowDefaultFormat",
        "title": "DateTime | Insert | Time Now | Default Format"
      },
      {
        "command": "DateTime.SelectFunction",
        "title": "DateTime | Select Function"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "DateTime",
      "properties": {
        "DateTime.DateFormat": {
          "type": "array",
          "default": [
            { "format": "YYYY/MM/DD(ddd)" },
            { "format": "ddd, DD MMM YYYY" },
            { "format": "dddd, DD MMMMM YYYY" },
            { "format": "YYYY/MM/DD(DDD)" },
            { "format": "YYYY年MM月DD日 DDDD" }
          ],
          "description": "Date Format"
        },
        "DateTime.DateTimeFormat": {
          "type": "array",
          "default": [
            { "format": "YYYY/MM/DD(ddd) HH:mm" },
            { "format": "ddd, DD MMM YYYY HH:mm" },
            { "format": "dddd, DD MMMMM YYYY" },
            { "format": "YYYY/MM/DD(DDD) HH:mm" },
            { "format": "YYYY年MM月DD日 DDDD AAAhh時mm分" }
          ],
          "description": "DateTime Format"
        },
        "DateTime.TimeFormat": {
          "type": "array",
          "default": [
            { "format": "HH:mm" },
            { "format": "hh:mm AA" },
            { "format": "AAAhh時mm分" }
          ],
          "description": "DateTime Format"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint --fix ./src",
    "pack": "vsce package && code --install-extension vscode-datetime-0.3.0.vsix",
    "": ""
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "vsce": "^2.5.3"
  }
}
