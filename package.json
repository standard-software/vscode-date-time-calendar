{
  "name": "vscode-datetime",
  "displayName": "DateTime",
  "description": "This extension outputs the date and time as formatted text",
  "version": "0.1.0",
  "publisher": "SatoshiYamamoto",
  "author": {
    "name": "Satoshi Yamamoto",
    "email": "standard.software.net@gmail.com",
    "url": "https://www.facebook.com/stndardsoftware"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-software/vscode-datetime"
  },
  "engines": {
    "vscode": "^1.62.3"
  },
  "categories": [
    "Other"
  ],
  "keywords": ["datetime"],
  "bugs": {
    "url": "https://github.com/standard-software/vscode-datetime/issues"
  },
  "main": "./src/extension.js",
  "activationEvents": [
    "onCommand:DateTime.InsertDateTodayDefaultFormat",
    "onCommand:DateTime.SelectFunction"
  ],
  "contributes": {
    "commands": [
      {
        "command": "DateTime.InsertDateTodayDefaultFormat",
        "title": "DateTime | Insert | Date Today | Default Format"
      },
      {
        "command": "DateTime.SelectFunction",
        "title": "DateTime | Select Function"
      }
    ],
    "configuration": {}
  },
  "scripts": {
    "lint": "eslint --fix ./src",
    "pack": "vsce package && code --install-extension vscode-datetime-0.1.0.vsix",
    "": ""
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "vsce": "^2.5.3"
  }
}
