{
  "name": "vscode-date-time-calendar",
  "displayName": "Date Time Calendar",
  "description": "This extension outputs the date and time and calendar as formatted text",
  "version": "0.4.0",
  "publisher": "SatoshiYamamoto",
  "author": {
    "name": "Satoshi Yamamoto",
    "email": "standard.software.net@gmail.com",
    "url": "https://www.facebook.com/stndardsoftware"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-software/vscode-date-time-calendar"
  },
  "engines": {
    "vscode": "^1.62.3"
  },
  "categories": [
    "Other"
  ],
  "keywords": ["datetime", "date", "time", "calendar"],
  "bugs": {
    "url": "https://github.com/standard-software/vscode-date-time-calendar/issues"
  },
  "main": "./src/extension.js",
  "activationEvents": [
    "onCommand:DateTimeCalendar.DateFormatDateTodayDefault",
    "onCommand:DateTimeCalendar.DateFormatDateTimeNowDefault",
    "onCommand:DateTimeCalendar.DateFormatTimeNowDefault",

    "onCommand:DateTimeCalendar.DateFormatDateTodaySelect",
    "onCommand:DateTimeCalendar.DateFormatDateTimeNowSelect",
    "onCommand:DateTimeCalendar.DateFormatTimeNowSelect",

    "onCommand:DateTimeCalendar.DateFormatDateYesterdayDefault",
    "onCommand:DateTimeCalendar.DateFormatDateTomorrowDefault",
    "onCommand:DateTimeCalendar.DateFormatDateYesterdaySelect",
    "onCommand:DateTimeCalendar.DateFormatDateTomorrowSelect",

    "onCommand:DateTimeCalendar.DateFormatSunSatWeekDaySelect",
    "onCommand:DateTimeCalendar.DateFormatMonSunWeekDaySelect",

    "onCommand:DateTimeCalendar.WeeklyCalenderSunSatThisWeekTodaySelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderSunSatLastWeekSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderSunSatThisWeekSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderSunSatNextWeekSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderSunSatLastToNextWeekSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderSunSatThisMonthSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderSunSatLastToNextMonthSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderSunSatThisYearSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderSunSatLastToNextYearSelect",

    "onCommand:DateTimeCalendar.WeeklyCalenderMonSunThisWeekTodaySelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderMonSunLastWeekSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderMonSunThisWeekSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderMonSunNextWeekSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderMonSunLastToNextWeekSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderMonSunThisMonthSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderMonSunLastToNextMonthSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderMonSunThisYearSelect",
    "onCommand:DateTimeCalendar.WeeklyCalenderMonSunLastToNextYearSelect",

    "onCommand:DateTimeCalendar.SelectFunction"
  ],
  "contributes": {
    "commands": [
      {
        "command": "DateTimeCalendar.DateFormatDateTodayDefault",
        "title": "Date Time Calendar | Date Format | Date Today | Default"
      },
      {
        "command": "DateTimeCalendar.DateFormatDateTimeNowDefault",
        "title": "Date Time Calendar | Date Format | DateTime Today Now | Default"
      },
      {
        "command": "DateTimeCalendar.DateFormatTimeNowDefault",
        "title": "Date Time Calendar | Date Format | Time Now | Default"
      },
      {
        "command": "DateTimeCalendar.DateFormatDateTodaySelect",
        "title": "Date Time Calendar | Date Format | Date Today | Select"
      },
      {
        "command": "DateTimeCalendar.DateFormatDateTimeNowSelect",
        "title": "Date Time Calendar | Date Format | DateTime Today Now | Select"
      },
      {
        "command": "DateTimeCalendar.DateFormatTimeNowSelect",
        "title": "Date Time Calendar | Date Format | Time Now | Select"
      },
      {
        "command": "DateTimeCalendar.DateFormatDateYesterdayDefault",
        "title": "Date Time Calendar | Date Format | Date Yesterday | Default"
      },
      {
        "command": "DateTimeCalendar.DateFormatDateTomorrowDefault",
        "title": "Date Time Calendar | Date Format | Date Tomorrow | Default"
      },
      {
        "command": "DateTimeCalendar.DateFormatDateYesterdaySelect",
        "title": "Date Time Calendar | Date Format | Date Yesterday | Select"
      },
      {
        "command": "DateTimeCalendar.DateFormatDateTomorrowSelect",
        "title": "Date Time Calendar | Date Format | Date Tomorrow | Select"
      },
      {
        "command": "DateTimeCalendar.DateFormatSunSatWeekDaySelect",
        "title": "Date Time Calendar | Date Format | Sun..Sat Last To Next Week | Select"
      },
      {
        "command": "DateTimeCalendar.DateFormatMonSunWeekDaySelect",
        "title": "Date Time Calendar | Date Format | Mon..Sun Last To Next Week | Select"
      },

      {
        "command": "DateTimeCalendar.WeeklyCalenderSunSatThisWeekTodaySelect",
        "title": "Date Time Calendar | Weekly Calender | Sun..Sat | This Week [Today] | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderSunSatLastWeekSelect",
        "title": "Date Time Calendar | Weekly Calender | Sun..Sat | Last Week | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderSunSatThisWeekSelect",
        "title": "Date Time Calendar | Weekly Calender | Sun..Sat | This Week | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderSunSatNextWeekSelect",
        "title": "Date Time Calendar | Weekly Calender | Sun..Sat | Next Week | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderSunSatLastToNextWeekSelect",
        "title": "Date Time Calendar | Weekly Calender | Sun..Sat | Last To Next 3Weeks [Today] | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderSunSatThisMonthSelect",
        "title": "Date Time Calendar | Weekly Calender | Sun..Sat | This Month | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderSunSatLastToNextMonthSelect",
        "title": "Date Time Calendar | Weekly Calender | Sun..Sat | Last To Next 3Month [Today] | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderSunSatThisYearSelect",
        "title": "Date Time Calendar | Weekly Calender | Sun..Sat | This Year [Today] | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderSunSatLastToNextYearSelect",
        "title": "Date Time Calendar | Weekly Calender | Sun..Sat | Last To Next 3Year [Today] | Select"
      },

      {
        "command": "DateTimeCalendar.WeeklyCalenderMonSunThisWeekTodaySelect",
        "title": "Date Time Calendar | Weekly Calender | Mon..Sun | This Week [Today] | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderMonSunLastWeekSelect",
        "title": "Date Time Calendar | Weekly Calender | Mon..Sun | Last Week | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderMonSunThisWeekSelect",
        "title": "Date Time Calendar | Weekly Calender | Mon..Sun | This Week | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderMonSunNextWeekSelect",
        "title": "Date Time Calendar | Weekly Calender | Mon..Sun | Next Week | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderMonSunLastToNextWeekSelect",
        "title": "Date Time Calendar | Weekly Calender | Mon..Sun | Last To Next 3Weeks [Today] | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderMonSunThisMonthSelect",
        "title": "Date Time Calendar | Weekly Calender | Mon..Sun | This Month | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderMonSunLastToNextMonthSelect",
        "title": "Date Time Calendar | Weekly Calender | Mon..Sun | Last To Next 3Month [Today] | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderMonSunThisYearSelect",
        "title": "Date Time Calendar | Weekly Calender | Mon..Sun | This Year [Today] | Select"
      },
      {
        "command": "DateTimeCalendar.WeeklyCalenderMonSunLastToNextYearSelect",
        "title": "Date Time Calendar | Weekly Calender | Mon..Sun | Last To Next 3Year [Today] | Select"
      },

      {
        "command": "DateTimeCalendar.SelectFunction",
        "title": "Date Time Calendar | Select Function"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "DateTimeCalendar",
      "properties": {
        "DateTimeCalendar.DateFormat": {
          "type": "array",
          "default": [
            { "format": "YYYY/MM/DD(ddd)" },
            { "format": "ddd, DD MMM YYYY" },
            { "format": "dddd, DD MMMMM YYYY" },
            { "format": "YYYY/MM/DD(DDD)" },
            { "format": "YYYY年MM月DD日 DDDD" }
          ],
          "description": "Date Format"
        },
        "DateTimeCalendar.DateTimeFormat": {
          "type": "array",
          "default": [
            { "format": "YYYY/MM/DD(ddd) HH:mm" },
            { "format": "ddd, DD MMM YYYY HH:mm" },
            { "format": "dddd, DD MMMMM YYYY" },
            { "format": "YYYY/MM/DD(DDD) HH:mm" },
            { "format": "YYYY年MM月DD日 DDDD AAAhh時mm分" }
          ],
          "description": "DateTime Format"
        },
        "DateTimeCalendar.TimeFormat": {
          "type": "array",
          "default": [
            { "format": "HH:mm" },
            { "format": "hh:mm AA" },
            { "format": "AAAhh時mm分" }
          ],
          "description": "DateTime Format"
        },
        "DateTimeCalendar.WeeklyCalendar": {
          "type": "array",
          "default": [
            {
              "title": "YYYY/MM|  DD(ddd)", "header": "YYYY/MM",
              "line": "  DD(ddd)", "today": " [DD(ddd)]"
            },
            {
              "title": "YYYY/MM|  DD  ddd", "header": "YYYY/MM",
              "line": "  DD  ddd", "today": " [DD] ddd"
            },
            {
              "title": "YYYY/MM|  DD(DDD)", "header": "YYYY/MM",
              "line": "  DD(DDD)", "today": " [DD(DDD)]"
            }
          ],
          "description": "DateTime Format"
        },
        "DateTimeCalendar.MonthlyCalendar": {
          "type": "array",
          "default": [
            {
              "title": "MMMMM YYYY ddd D \"Space2\"",
              "header": "LMMMMM                YYYY", "dayOfWeek": "ddd", "date": "SD",
              "indent": "  ", "space": "  ",
              "todayLeft": "[", "todayRight": "]",
              "otherMonthDate": false
            },
            {
              "title": "YYYY/MM ddd DD \"Space2 OtherMonth\"",
              "header": "MM YYYY", "dayOfWeek": "ddd", "date": "DD",
              "indent": "  ", "space": "  ",
              "todayLeft": "[", "todayRight": "]",
              "otherMonthDate": true
            },
            {
              "title": "YYYY MMMMM dd D \"Space1\"",
              "header": "YYYY         RMMMMM", "dayOfWeek": "dd", "date": "SD",
              "indent": "  ", "space": " ",
              "todayLeft": "[", "todayRight": "]",
              "otherMonthDate": false
            },
            {
              "title": "YYYY/MM ddd DD \"Space1 OtherMonth\"",
              "header": "YYYY/MM", "dayOfWeek": "ddd", "date": "DD",
              "indent": "  ", "space": " ",
              "todayLeft": "", "todayRight": "<",
              "otherMonthDate": true
            },
            {
              "title": "YYYY年MM月 DDD DD \"Space2 OtherMonth\"",
              "header": "YYYY年MM月", "dayOfWeek": "DDD", "date": "DD",
              "indent": "", "space": "  ",
              "todayLeft": "", "todayRight": "<-",
              "otherMonthDate": true
            }
          ],
          "description": "DateTime Format"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint --fix ./src",
    "pack": "vsce package && code --install-extension vscode-date-time-calendar-0.4.0.vsix",
    "": ""
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "vsce": "^2.5.3"
  }
}
